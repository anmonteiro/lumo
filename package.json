{
  "name": "lumo",
  "version": "1.0.0",
  "description": "Fast, cross-platform, standalone ClojureScript REPL",
  "author": "anmonteiro",
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/anmonteiro/lumo.git"
  },
  "bugs": {
    "url": "https://github.com/anmonteiro/lumo/issues"
  },
  "homepage": "https://github.com/anmonteiro/lumo",
  "scripts": {
    "type:check": "flow check",
    "lint": "eslint src/js",
    "test": "jest",
    "bundle": "node scripts/build.js --dev",
    "dev": "node target/bundle.js -c target -k lumo-cache"
  },
  "main": "./src/js/index.js",
  "dependencies": {
    "jszip": "2.6.1",
    "parinfer": "2.0.0",
    "posix-getopt": "github:anmonteiro/node-getopt#master"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "derequire": "^2.0.3",
    "envify": "^4.0.0",
    "eslint": "^3.11.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.28.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "flow-bin": "^0.36.0",
    "jest-cli": "^17.0.3",
    "nexe": "github:anmonteiro/nexe#master",
    "uglify-js": "^2.7.4"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "scripts/jest/environment.js"
    ],
    "collectCoverageFrom": [
      "src/js/**/*.js"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/lib",
      "<rootDir>/node_modules"
    ],
    "testPathDirs": [
      "src/js"
    ]
  }
}
